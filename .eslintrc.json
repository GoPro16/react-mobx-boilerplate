{
  "extends": "airbnb",
  "parser":"babel-eslint",
  "env": {
    "es6":true,
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "experimentalDecorators":true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "experimentalDecorators":true,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "import/no-extraneous-dependencies":0,
    "import/first":0,
    "import/order":2,
    "import/no-unresolved":0,
    "import/extensions":["off","never"],
    "indent":[
      "warn",
      4,
      {
        "SwitchCase":1
      }
    ],
    "react/jsx-indent":["warn",4],
    "react/jsx-index-props":["warn",4],
    "no-underscore-dangle":0,
    "comma-dangle":[
      "error",
      {
        "extentions":[".js"]
      }
    ],
    "max-len":["warn",200],
    "func-names":0,
    "no-console":"warn",
    "react/jsx-filename-extensions":[
      1,
      {
        "extensions":[".js"]
      }
    ],
    "react-forbid-prop-types":0,
    "jsx-a11y/imge-has-alt":0,
    "jsx-a11y/no-static-element-interactions":0,
    "jsx-a11y-label-has-for":0,
    "no-magic-numbers":0,
    "linebreak-style":[0,"unix"],
    "react/prop-types":0,
    "react/no-array-index-key":0,
    "arrow-body-style":[
      "warn",
      "as-needed"
    ],
    "arrow-parens":0
  }
}